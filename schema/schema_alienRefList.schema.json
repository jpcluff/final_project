{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Alien Ref Overview List",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "alienValidated": {
          "type": "boolean"
        },
        "creators": {
          "type": "string"
        },
        "sourceType": {
          "enum": ["book","film","tv","game","comic","other"]
        },
        "summary": {
          "type": "string"
        },
        "imgOverview": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "alienValidated",
        "summary",
        "imgOverview"
      ]
    }
  }
  